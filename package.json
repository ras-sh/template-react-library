{
  "name": "@ras-sh/template-react-library",
  "version": "0.0.2",
  "type": "module",
  "description": "Template for React library projects.",
  "keywords": [],
  "homepage": "https://ras.sh",
  "bugs": {
    "url": "https://github.com/ras-sh/template-react-library/issues",
    "email": "hi@ras.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ras-sh/template-react-library"
  },
  "license": "MIT",
  "author": {
    "name": "Richard Solomou",
    "email": "hi@ras.sh",
    "url": "https://ras.sh"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./button": {
      "import": {
        "types": "./dist/button.d.ts",
        "default": "./dist/button.js"
      },
      "require": {
        "types": "./dist/button.d.cts",
        "default": "./dist/button.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check-types": "tsc --noEmit",
    "check": "ultracite check",
    "fix": "ultracite fix --unsafe",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prepublishOnly": "pnpm check && pnpm check-types && pnpm test && pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@changesets/cli": "^2.29.7",
    "@chromatic-com/storybook": "^4.1.1",
    "@ras-sh/typescript-config": "^0.0.2",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "lefthook": "^1.13.6",
    "playwright": "^1.56.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.13",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "ultracite": "5.6.4",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
